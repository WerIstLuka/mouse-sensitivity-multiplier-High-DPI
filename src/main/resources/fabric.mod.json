{
	"schemaVersion": 1,
	"id": "mouse-sensitivity-multiplier-high-dpi",
	"version": "${version}",
	"name": "Mouse Sensitivity Multiplier high dpi",
	"description": "A lightweight Minecraft mod that applies a customizable multiplier to your existing mouse sensitivity settings. Modified for 8k dpi",
	"authors": [
		"tropicbliss",
		"WerIstLuka"
	],
	"contact": {
		"homepage": "https://github.com/WerIstLuka/mouse-sensitivity-multiplier-High-DPI",
		"sources": "https://github.com/WerIstLuka/mouse-sensitivity-multiplier-High-DPI"
	},
	"license": "MIT",
	"icon": "assets/mouse-sensitivity-multiplier/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"net.tropicbliss.MouseSensitivityMultiplier"
		]
	},
	"accessWidener": "mouse-sensitivity-multiplier.accesswidener",
	"mixins": [
		"mouse-sensitivity-multiplier.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	}
}
